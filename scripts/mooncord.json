{
    "connection": {
        "moonraker_socket_url": "ws://127.0.0.1/websocket",
        "moonraker_url": "http://127.0.0.1",
        "moonraker_token": "",
        "moonraker_database": "MC_SERVICE",
        "bot_token": ""
    },
    "status": {
        "update_interval": 10,
        "use_percent": true,
        "min_interval": 15
    },
    "language": {
        "messages": "en",
        "command_syntax": "en",
        "buttons_use_syntax_locale": false
    },
    "permission": {
        "controllers": {
            "roles": [],
            "users": ""
        }
    },
    "webcam": {
        "url": "http://127.0.0.1/webcam/?action=snapshot",
        "quality": 80,
        "rotation": 0,
        "brightness": 0,
        "contrast": 0,
        "vertical_mirror": false,
        "horizontal_mirror": false,
        "greyscale": false,
        "sepia": false
    },
    "timelapse": {
        "enable": false,
        "framerate": 10,
        "post_at_print_end": false,
        "ffmpeg_codec": "libx264",
        "ffmpeg_arguments": "-pix_fmt yuv420p -preset slower -crf 30"
    }
}