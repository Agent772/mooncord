    "commands": {
        "admin": {
            "description": "Manage Admin Role or User.",
            "command": "admin",
            "options": {
                "role": {
                    "name": "role",
                    "description": "Modify Role.",
                    "options": {
                        "role": {
                            "name": "role",
                            "description": "Select a Role."
                        }
                    }
                },
                "user": {
                    "name": "user",
                    "description": "Modify a User.",
                    "options": {
                        "user": {
                            "name": "user",
                            "description": "Select a User."
                        }
                    }
                }
            }
        },
        "dump": {
            "description": "dump cache.",
            "command": "dump",
            "options": {
                "section": {
                    "name": "section",
                    "description": "Select the Section to dump.",
                    "choices": {
                        "cache": "Cache",
                        "database": "Database"
                    }
                }
            }
        },
        "reset_database": {
            "description": "clear the Database.",
            "command": "reset"
        },
        "editchannel": {
            "description": "Add or Remove broadcast channel.",
            "command": "editchannel",
            "options": {
                "channel": {
                    "name": "channel",
                    "description": "Select a Channel to add/remove it as Broadcast channel."
                } 
            }
        },
        "emergency_stop": {
            "description": "Emergency Stop the Printer?",
            "command": "emergencystop"
        },
        "execute": {
            "description": "Execute a GCode Command.",
            "command": "execute",
            "options": {
                "gcode": {
                    "name": "gcode",
                    "description": "GCode that you want to execute."
                }
            }
        },
        "fileinfo": {
            "description": "Shows Informations about a Print File.",
            "command": "fileinfo",
            "options": {
                "file": {
                    "name": "file",
                    "description": "Specifies a Print File."
                }
            }
        },
        "get_log": {
            "description": "Retrieve a Log.",
            "command": "getlog",
            "options": {
                "log_file": {
                    "name": "logfile",
                    "description": "Specifies the Log File."
                }
            }
        },
        "get_user_id": {
            "description": "Retrieve the User ID.",
            "command": "getuserid",
            "options": {
                "user": {
                    "name": "user",
                    "description": "Specifies a User."
                }
            }
        },
        "restart": {
            "description": "Restart a Service.",
            "command": "restart",
            "options": {
                "service": {
                    "name": "service",
                    "description": "Specifies a Service."
                }
            }
        },
        "info": {
            "description": "Send a Description about me.",
            "command": "info"
        },
        "listfiles": {
            "description": "List all Print Files.",
            "command": "listfiles"
        },
        "systeminfo": {
            "description": "Get the current System Informations.",
            "command": "systeminfo",
            "options": {
                "component": {
                    "name": "component",
                    "description": "Select the component you want to know the information about."
                }
            }
        },
        "notify": {
            "description": "Should i DM you with the current print status?",
            "command": "notifyme"
        },
        "printjob": {
            "description": "Control or start a Print Job.",
            "command": "printjob",
            "options": {
                "pause": {
                    "name": "pause",
                    "description": "Pause Print Job"
                },
                "cancel": {
                    "name": "cancel",
                    "description": "Cancel Print Job"
                },
                "resume": {
                    "name": "resume",
                    "description": "Resume Print Job"
                },
                "start": {
                    "name": "start",
                    "description": "Start new Print Job",
                    "options": {
                        "file": {
                            "name": "file",
                            "description": "Select a Print File."
                        }
                    }
                }
            }
        },
        "status": {
            "description": "Get the current Print Status.",
            "command": "status"
        },
        "temp": {
            "description": "Get the current Temperatures from Klipper.",
            "command": "temp"
        },
        "timelapse": {
            "description": "Get the latest Timelapse.",
            "command": "timelapse"
        }
